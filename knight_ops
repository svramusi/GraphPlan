
(operator
 move
 (params (<x> Knight) (<y> Space) (<z> Space))
 (preconds
  (on <x> <y>))
 (effects
  (on <x> <z>) (del on <x> <y>)))

(operator
 move_a1_b3
 (params (<x> Knight))
 (preconds
  (on <x> a1))
 (effects
  (on <x> b3)))

