(whiteKnight Knight)

(preconds
 (on whiteKnight b4)
 (empty a1) (empty a2) (empty a3) (empty a4)
 (empty b1) (empty b2) (empty b3)
 (empty c1) (empty c2) (empty c3) (empty c4)
 (empty d1) (empty d2) (empty d3) (empty d4))

(effects
 (on whiteKnight a4) (not-on whiteKnight b4)
 (empty a1) (empty a2) (empty a3) (not-empty a4)
 (empty b1) (empty b2) (empty b3) (empty b4)
 (empty c1) (empty c2) (empty c3) (empty c4)
 (empty d1) (empty d2) (empty d3) (empty d4))
